{
    "name": "veronalabs/books-management",
    "description": "A WordPress plugin for managing book information with custom post types, taxonomies, and Rabbit Framework integration.",
    "type": "wordpress-plugin",
    "autoload": {
        "psr-4": {
            "BooksManager\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Hooman Helmi"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "veronalabs/rabbit": "^1.5"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.10",
        "wp-coding-standards/wpcs": "^3.1",
        "phpmd/phpmd": "^2.15"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
